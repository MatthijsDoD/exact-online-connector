{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azureTestApiExact"
		},
		"myBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'myBlobStorage'"
		},
		"my_linked_sql_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'my_linked_sql_connection'"
		},
		"http_exactonline_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://start.exactonline.nl/"
		},
		"rest_exactonline_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://start.exactonline.nl/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azureTestApiExact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"myBlobStorage_connectionString": {
						"value": "[parameters('myBlobStorage_connectionString')]"
					},
					"my_linked_sql_connection_connectionString": {
						"value": "[parameters('my_linked_sql_connection_connectionString')]"
					},
					"http_exactonline_properties_typeProperties_url": {
						"value": "[parameters('http_exactonline_properties_typeProperties_url')]"
					},
					"rest_exactonline_properties_typeProperties_url": {
						"value": "[parameters('rest_exactonline_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}